{
  "name": "etl_dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm:dev:docker\" \"pnpm:dev:server\" \"pnpm:dev:client\" --names \"DOCKER,SERVER,CLIENT\" --prefix-colors \"blue,green,cyan\"",
    "dev:docker": "docker-compose up",
    "dev:server": "cd server && pnpm run dev",
    "dev:client": "vite",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "build": "pnpm run build:client && pnpm run build:server",
    "build:client": "tsc -b && vite build",
    "build:server": "cd server && pnpm run build",
    "start:prod": "concurrently \"pnpm:docker:up\" \"pnpm:start:server\" \"pnpm:start:client\" --names \"DOCKER,SERVER,CLIENT\" --prefix-colors \"blue,green,cyan\"",
    "start:server": "cd server && pnpm run start",
    "start:client": "vite preview --host --port 4173",
    "lint": "eslint .",
    "clean": "rm -rf dist server/dist node_modules server/node_modules"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@tanstack/react-query": "^5.90.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.564.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.13.0",
    "tailwind-merge": "^3.4.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/xlsx": "^0.0.36",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.24",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.3.1"
  }
}
